			"audioName" : "NONE",
			"cameraFocusGuid" : "",
			"cameraDistance" : "Far",
			"cameraHeight" : "Default",
			"revealRadius" : -1
		},
		{
			"words": "Your slow pig of ship is an easy target and you can't stop all that we have aimed at you.",
			"wordsColor": {
				"r": 1,
				"g": 1,
				"b": 1,
