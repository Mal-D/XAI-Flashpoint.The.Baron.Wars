			"audioName" : "NONE",
			"cameraFocusGuid" : "",
			"cameraDistance" : "Far",
			"cameraHeight" : "Default",
			"revealRadius" : -1
			},
		{
			"words" : "Sorry about that Commander, but they keep changing their penetration strategy.",
			"wordsColor" : {
				"r" : 1,
				"g" : 1,
				"b" : 1,
				"a" : 1
	    	},
