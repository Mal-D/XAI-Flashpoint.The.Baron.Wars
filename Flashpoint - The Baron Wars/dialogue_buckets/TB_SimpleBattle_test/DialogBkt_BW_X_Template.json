			"audioName" : "NONE",
			"cameraFocusGuid" : "",
			"cameraDistance" : "Far",
			"cameraHeight" : "Default",
			"revealRadius" : -1
		},
		{
			"words" : "Your feeble attempts to deny us will only lead to failure.",
			"wordsColor" : {
				"r" : 1,
				"g" : 1,
				"b" : 1,
				"a" : 1
	    	},
