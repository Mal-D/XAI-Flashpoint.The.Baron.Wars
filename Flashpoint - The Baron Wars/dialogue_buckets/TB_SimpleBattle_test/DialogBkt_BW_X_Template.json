			"cameraHeight": "Default",
			"revealRadius": -1
		},
        {
            "words" : "/// HARDENED ENCRYPTION PROTCOL ENABLED ///",
            "wordsColor" : {
                "r" : 1,
                "g" : 1,
                "b" : 1,
                "a" : 1
